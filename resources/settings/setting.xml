<settings>
    <setting propertyKey="@Properties.Username" title="@Strings.UsernameTitle">
        <settingConfig type="alphaNumeric" required="true"></settingConfig>
    </setting>
    <setting propertyKey="@Properties.Password" title="@Strings.PasswordTitle">
        <settingConfig type="password" required="true"></settingConfig>
    </setting>
